<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:conv="clr-namespace:WPFShared.Converters"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<conv:VisiblityToBooleanConverter
		x:Key="ConverterVisibilityToBoolean" />
	<conv:BooleanNegateConverter
		x:Key="ConverterBooleanNegate" />
	<conv:BooleanToVisibilityConverter
		x:Key="ConverterBooleanToVisibility" />
	<conv:ByteWholeSizeConverter
		x:Key="ConverterByteWholeSize" />
	<conv:ColorTransitionConverter
		x:Key="ConverterColorTransition" />
	<conv:EmptyCollectionToVisibilityConverter
		x:Key="ConverterEmptyCollectionToVisibility" />
	<conv:EmptyCollectionToBooleanConverter
		x:Key="ConverterEmptyCollectionToBoolean" />
	<conv:EnumToBooleanConverter
		x:Key="ConverterEnumToBoolean" />
	<conv:EnumToVisibilityConverter
		x:Key="ConverterEnumToVisibility" />
	<conv:InListToVisibilityConverter
		x:Key="ConverterInListToVisibility" />
	<conv:InRangeToBooleanConverter
		x:Key="ConverterInRangeToBoolean" />
	<conv:InRangeToVisibilityConverter
		x:Key="ConverterInRangeToVisibility" />
	<conv:MultipleBooleanToVisibilityConverter
		x:Key="ConverterMultipleBooleanToVisibility" />
	<conv:MultipleObjectConverter
		x:Key="ConverterMultipleObject" />
	<conv:NullToBooleanConverter
		x:Key="ConverterNullToBoolean" />
	<conv:NullToVisibilityConverter
		x:Key="ConverterNullToVisibility" />
	<conv:SecondsRemainingToStringConverter
		x:Key="ConverterSecondsRemainingToString" />
	<conv:StringConcatConverter
		x:Key="ConverterStringConcat" />
	<conv:StringFormatConverter
		x:Key="ConverterStringFormat" />
	<conv:StringNullOrEmptyToVisibilityConverter
		x:Key="ConverterStringNullOrEmptyToVisibility" />
	<conv:TestConverter
		x:Key="ConverterTest" />
	<Style
		x:Key="LinkButtonStyle"
		TargetType="{x:Type Button}">
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{x:Type Button}">
					<TextBlock>
						<ContentPresenter />
					</TextBlock>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter
			Property="Foreground"
			Value="#FF0000FF" />
		<Setter
			Property="FrameworkElement.Cursor"
			Value="Hand" />
		<Style.Triggers>
			<Trigger
				Property="UIElement.IsMouseOver"
				Value="True">
				<Setter
					Property="Foreground"
					Value="#FFFF0000" />
			</Trigger>
		</Style.Triggers>
	</Style>
</ResourceDictionary>
